<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LocalBusinessExplorer.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LocalBusinessExplorer"
    Shell.FlyoutBehavior="Disabled"
    Title="LocalBusinessExplorer">

    <ShellContent
        <ShellContent Title="Home" ContentTemplate="{DataTemplate local:HomePage}" />
        <ShellContent Title="Login" ContentTemplate="{DataTemplate local:LoginPage}" />
        <ShellContent Title="Sign Up" ContentTemplate="{DataTemplate local:SignUpPage}" />


</Shell>
